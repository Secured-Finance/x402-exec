{
  "name": "@secured-finance/x402-fetch",
  "version": "0.3.0",
  "description": "Fetch wrapper for x402x settlement framework",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "format": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --write \"**/*.{ts,js,cjs,json,md}\"",
    "format:check": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --check \"**/*.{ts,js,cjs,json,md}\""
  },
  "keywords": [
    "x402",
    "x402x",
    "fetch",
    "payment",
    "settlement"
  ],
  "author": "Secured Finance",
  "license": "Apache-2.0",
  "dependencies": {
    "@secured-finance/x402-core": "workspace:*",
    "viem": "2.40.3",
    "x402": "npm:sf-x402x@^0.7.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^9.0.0",
    "prettier": "3.5.2",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^1.0.0"
  }
}
